FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
type    massTransferMultiphaseSystem;

phases  (liquid solid);

liquid
{
    type            pureMovingPhaseModel;
}

solid
{
    type            pureStaticSolidPhaseModel;
}

interfacePorous
(
    (solid and liquid)
    {
        type            VollerPrakash;
        solidPhase      alpha.solid;
        Cu              1e12;
    }
);

massTransferModel
(
    (liquid to solid)
    {
        type            precipitate;
        C               1E-5;
        Tactivate       310;
    	Cactivate   	1;
	    Mv		        18;
        alphaSolidMin   0.9;
    }
);

// ************************************************************************* //
