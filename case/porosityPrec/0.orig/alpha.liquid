FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      alpha.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 1; 
 
boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    sphere1
    {
        type            fixedValue;
        value           uniform 0;
    }
    sphere2
    {
        type            fixedValue;
        value           uniform 0;
    }
    sphere3
    {
        type            fixedValue;
        value           uniform 0;
    }
    sphere4
    {
        type            fixedValue;
        value           uniform 0;
    }
    periodicWall
    {
        type            symmetry;
    }
    "proc.*"
    {
        type    processor;
    }
}


