FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      areaDensityGrad;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 0 0 0 0];


internalField   uniform 0; 
 
boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    sphere
    {
        type            zeroGradient;
    }
    periodicWall
    {
        type            symmetry;
    }
}


