FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 5;

boundaryField
{
    outlet
    {
        type           zeroGradient;
    }
    inlet
    {
        type           fixedValue;
        value          uniform 5;

    }
    wall
    {
        type           zeroGradient;
    }
    sphere
    {
        type           zeroGradient;
    }
