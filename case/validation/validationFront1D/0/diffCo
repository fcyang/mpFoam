{
    inlet
    {
        type           zeroGradient;
    }
    outlet
    {
        type           zeroGradient;

    }
    top
    {
        type           empty;
    }
    bottom
    {
        type           empty;
    }
    frontAndBack
    {
        type           empty;
    }
